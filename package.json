{
  "name": "cli-ts-signer",
  "version": "1.0.0",
  "description": "ts based signer for moonbeam",
  "main": "index.js",
  "repository": "git@github.com:PureStake/cli-ts-signer.git",
  "author": "Antoine Estienne <estienne.antoine@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/elliptic": "^6.4.12",
    "@types/node": "^15.6.1"
  },
  "scripts": {
    "cli": "npx ts-node index.ts",
    "lint": "prettier -w .",
    "compileJs": "npx tsc --outDir compiledJs",
    "package-moon-key": "node_modules/.bin/tsc -p tsconfig.json; node_modules/.bin/pkg --debug -t node12-linux-x64,node12-win-x64 try.js; rm try.js"
  },
  "dependencies": {
    "@polkadot/api": "^4.15.1",
    "@polkadot/keyring": "^6.9.1",
    "@polkadot/types": "^4.15.1",
    "@polkadot/util": "^6.9.1",
    "@polkadot/wasm-crypto": "^4.0.2",
    "@types/prompts": "^2.0.13",
    "@types/yargs": "^17.0.1",
    "elliptic": "^6.5.4",
    "moonbeam-types-bundle": "^1.1.22",
    "pkg": "^5.3.0",
    "prompts": "^2.4.1",
    "typescript": "^4.3.5",
    "yargs": "^17.0.1"
  },
  "pkg": {
    "scripts": [
      "methods/*.js"
    ],
    "targets": [
      "node14-macos"
    ],
    "outputPath": "binary"
  }
}
